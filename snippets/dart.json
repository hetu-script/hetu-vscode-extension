{
  "External Class Binding": {
    "prefix": "extc",
    "body": [
      "extension ${1:}Binding on ${1:} {",
      "  dynamic htFetch(String varName) {",
      "    switch (varName) {",
      "      case 'instanceMethod':",
      "        return (HTNamespace context,",
      "                {List<dynamic> positionalArgs = const [],",
      "                Map<String, dynamic> namedArgs = const {},",
      "                List<HTType> typeArgs = const []}) {",
      "                  method(positionalArgs[0]);",
      "                }",
      "      case 'getter':",
      "        return value",
      "      default:",
      "        throw HTError.undefined(varName);",
      "    }",
      "  }",
      "",
      "  void htAssign(String varName, dynamic varValue) {",
      "    switch (varName) {",
      "      case 'setter':",
      "        value = varValue;",
      "        break;",
      "      default:",
      "        throw HTError.undefined(varName);",
      "    }",
      "  }",
      "}",
      "",
      "class ${1:}ClassBinding extends HTExternalClass {",
      "  ${1:}ClassBinding() : super('${1:}');",
      "",
      "  @override",
      "  dynamic memberGet(String varName) {",
      "    switch (varName) {",
      "      // constructor",
      "      case '${1:}'",
      "        return (HTNamespace context,",
      "                {List<dynamic> positionalArgs = const [],",
      "                Map<String, dynamic> namedArgs = const {},",
      "                List<HTType> typeArgs = const []}) {",
      "                  return ${1:}();",
      "                }",
      "      case '${1:}'.staticMethod",
      "        return (HTNamespace context,",
      "                {List<dynamic> positionalArgs = const [],",
      "                Map<String, dynamic> namedArgs = const {},",
      "                List<HTType> typeArgs = const []}) {",
      "                  ${1:}.staticMethod(positionalArgs[0]);",
      "                }",
      "      case '${1:}'.staticGetter",
      "        return (HTNamespace context,",
      "                {List<dynamic> positionalArgs = const [],",
      "                Map<String, dynamic> namedArgs = const {},",
      "                List<HTType> typeArgs = const []}) {",
      "                  ${1:}.staticMethod(positionalArgs[0]);",
      "                }",
      "      default:",
      "        throw HTError.undefined(varName);",
      "    }",
      "  }",
      "",
      "  @override",
      "  void memberSet(String varName, dynamic varValue) {",
      "    switch (varName) {",
      "      case '${1:}.staticSetter':",
      "        ${1:}.staticField = varValue;",
      "        break;",
      "      default:",
      "        throw HTError.undefined(varName);",
      "    }",
      "  }",
      "",
      "  @override",
      "  dynamic instanceMemberGet(dynamic object, String varName) {",
      "    var i = object as ${1:};",
      "    return i.htFetch(varName);",
      "  }",
      "",
      "  @override",
      "  void instanceMemberSet(dynamic object, String varName, dynamic varValue) {",
      "    var i = object as ${1:};",
      "    i.htAssign(varName, varValue);",
      "  }",
      "}"
    ],
    "description": "External Class Binding"
  },
